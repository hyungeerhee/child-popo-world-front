{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "dist",
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://52.78.53.247:8080/$1",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Credentials": "true",
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
          "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization, refresh-token"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "VITE_API_URL": "http://52.78.53.247:8080",
      "VITE_API_TIMEOUT": "5000",
      "VITE_ENV": "production"
    }
  } 